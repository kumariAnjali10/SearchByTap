{
    "manifest_version": 3,
    "name": "SearchByTap Chrome Extension",
    "version": "1.0",
    "permissions": ["activeTab", "storage"],
    "action": {
      "default_popup": "index.html"      
    },
    "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self'"
    }
  }
  